<launch>
    <!-- Start tilt joint controller -->
    <rosparam file="$(find dynamixel_tut)/config/tilt.yaml" command="load"/>
    <node name="tilt_controller_spawner" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=dxl_manager
                --port pan_tilt_port
                tilt_controller"
          output="screen"/>

    <!-- Start pan joint controller -->
    <rosparam file="$(find dynamixel_tut)/config/pan.yaml" command="load"/>
    <node name="pan_controller_spawner" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=dxl_manager
                --port pan_tilt_port
                pan_controller" 
          output="screen"/>
</launch>